        <parameter name="addFirstName" value="Bola"></parameter>
        <parameter name="addLastName" value="Bak"></parameter>

        <parameter name="addPostCode" value="60000"></parameter>
        <parameter name="alertMessage1" value="Customer added successfully"></parameter>

        <parameter name="openName" value="Harry Potter"></parameter>
        <parameter name="openCurrency" value="Dollar"></parameter>

        <parameter name="alertMessage2" value="Account created successfully"></parameter>
